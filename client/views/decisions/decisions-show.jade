h1#dashboardTitle Result Details
.row
  .col-xs-4
    h3 Decision Title
    h3 {{decision.title}}
  .col-xs-4
    h3 Decision Date Taken
    h3 {{decision.date | date:'yyy-MM-dd'}}
  .col-xs-4
    h3 Decision Results
    h3 {{decision.result}}
.row
  .col-xs-8.col-xs-offset-2
      table.table-striped.table-hover
        thead
          tr
            th: h3 Number
            th: h3 Choice A
            th: h3 Choice B
            th: h3 User's Choice
          tr(ng-repeat='choice in choices track by $index')
            td: h3 {{$index + 1}}
            td: h3 {{choice.a}}
            td: h3 {{choice.b}}
            td: h3 {{choice.user}}
